{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://example.com/mcp/file.estimateFilename.params.json",
    "title": "file.estimateFilename params",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "fileType": {
            "type": "string"
        },
        "columns": {
            "type": "integer",
            "minimum": 1
        },
        "rows": {
            "type": "integer",
            "minimum": 0
        },
        "header": {
            "type": "string",
            "enum": [
                "H",
                "NH"
            ]
        },
        "validity": {
            "type": "string",
            "enum": [
                "V",
                "I"
            ]
        },
        "timestamp": {
            "type": "string",
            "pattern": "^\\d{8}_[0-2]\\d[0-5]\\d[0-5]\\d$"
        },
        "extension": {
            "type": "string",
            "enum": [
                ".csv",
                ".txt"
            ]
        }
    },
    "required": [
        "fileType",
        "columns",
        "rows",
        "header",
        "validity"
    ]
}