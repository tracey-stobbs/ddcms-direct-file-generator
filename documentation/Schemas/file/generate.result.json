{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://example.com/mcp/file.generate.result.json",
    "title": "file.generate result",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "filePath": {
            "type": "string"
        },
        "fileContent": {
            "type": "string"
        },
        "meta": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "rows": {
                    "type": "integer",
                    "minimum": 0
                },
                "columns": {
                    "type": "integer",
                    "minimum": 1
                },
                "header": {
                    "type": "string",
                    "enum": [
                        "H",
                        "NH"
                    ]
                },
                "validity": {
                    "type": "string",
                    "enum": [
                        "V",
                        "I"
                    ]
                },
                "extension": {
                    "type": "string",
                    "enum": [
                        ".csv",
                        ".txt"
                    ]
                },
                "fileType": {
                    "type": "string"
                },
                "sun": {
                    "type": "string"
                }
            },
            "required": [
                "rows",
                "columns",
                "header",
                "validity",
                "fileType",
                "sun"
            ]
        },
        "persisted": {
            "type": "boolean"
        },
        "persistedPath": {
            "type": "string"
        }
    },
    "required": [
        "filePath",
        "meta"
    ]
}