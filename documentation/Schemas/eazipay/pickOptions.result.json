{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://example.com/mcp/eazipay.pickOptions.result.json",
    "title": "eazipay.pickOptions result",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "options": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "dateFormats": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "enum": [
                            "YYYY-MM-DD",
                            "DD-MMM-YYYY",
                            "DD/MM/YYYY"
                        ]
                    }
                },
                "trailerFormats": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "enum": [
                            "quoted",
                            "unquoted"
                        ]
                    }
                },
                "examples": {
                    "type": "object"
                }
            },
            "required": [
                "dateFormats",
                "trailerFormats",
                "examples"
            ]
        }
    },
    "required": [
        "options"
    ]
}